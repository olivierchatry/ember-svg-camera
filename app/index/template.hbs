
<div class="fullsize">
	{{#svg-viewer as |viewWidth viewHeight|}}
		{{#svg-viewport viewWidth=viewWidth viewHeight=viewHeight }}
			{{#svg-stage viewWidth=viewWidth viewHeight=viewHeight}}
				<g transform="translate(1050 1050)">
					<image xlink:href="/images/01.jpg" width="32" height="24"/>
				</g>

				<circle
					cx=1000
					cy=1000
					r=10
					fill="black"
				/>
			{{/svg-stage}}
		{{/svg-viewport}}
	{{/svg-viewer}}

	{{outlet}}
</div>
